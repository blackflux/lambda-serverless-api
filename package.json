{
  "name": "lambda-serverless-api",
  "description": "Basic Api Wrapper for Serverless Framework.",
  "main": "lib/index.js",
  "keywords": [
    "serverless",
    "lambda",
    "api",
    "api gateway",
    "rate limiter",
    "monitoring",
    "logging"
  ],
  "author": "Lukas Siemon",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/register": "7.4.4",
    "@blackflux/eslint-plugin-rules": "1.0.18",
    "@blackflux/robo-config-plugin": "1.10.5",
    "app-root-path": "2.2.1",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "js-gardener": "2.0.35",
    "lambda-tdd": "2.7.10",
    "nock": "10.0.6",
    "nyc": "14.1.1",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "semantic-release": "15.13.12"
  },
  "version": "0.0.0-development",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --copy-files --include-dotfiles --config-file ./.babelrc",
    "build-clean": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-clean && npx semantic-release",
    "gardener": "node gardener"
  },
  "homepage": "https://github.com/blackflux/lambda-serverless-api#readme",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/lambda-serverless-api/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 6.10.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/lambda-serverless-api.git"
  },
  "bugs": {
    "url": "https://github.com/blackflux/lambda-serverless-api/issues"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@hapi/joi": "15.0.3",
    "joi-strict": "1.0.2",
    "js-yaml": "3.13.1",
    "lambda-rate-limiter": "2.4.16",
    "lambda-rollbar": "1.10.13",
    "lodash.defaultsdeep": "4.6.0",
    "lodash.difference": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.set": "4.3.2",
    "lodash.xor": "4.5.0",
    "moment": "2.24.0",
    "object-fields": "1.3.8",
    "object-scan": "10.0.3",
    "route-recognizer": "0.3.4",
    "swagger-parser": "6.0.5",
    "yaml-boost": "1.9.7"
  },
  "peerDependencies": {},
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
