{
  "name": "lambda-serverless-api",
  "description": "Basic Api Wrapper for Serverless Framework.",
  "main": "lib/index.js",
  "keywords": [
    "serverless",
    "lambda",
    "api",
    "api gateway",
    "rate limiter",
    "monitoring",
    "logging"
  ],
  "author": "Lukas Siemon",
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/register": "7.12.1",
    "@blackflux/eslint-plugin-rules": "1.3.41",
    "@blackflux/robo-config-plugin": "3.10.14",
    "babel-eslint": "10.1.0",
    "babel-preset-latest-node": "5.1.1",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "eslint": "7.13.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-mocha": "8.0.0",
    "js-gardener": "2.0.180",
    "lambda-tdd": "2.13.11",
    "nock": "13.0.5",
    "node-tdd": "2.18.11",
    "nyc": "15.1.0",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "semantic-release": "17.2.3",
    "smart-fs": "1.12.1"
  },
  "version": "0.0.0-development",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --copy-files --include-dotfiles --config-file ./.babelrc",
    "build-clean": "yarn run clean && yarn run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "yarn run clean && yarn run gardener && yarn run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "yarn run build-clean && npx semantic-release",
    "gardener": "node gardener",
    "docker": "docker run --net host -u`id -u`:`id -g` -v $(pwd):/user/project -v ~/.aws:/user/.aws -v ~/.npmrc:/user/.npmrc -w /user/project -it --entrypoint /bin/bash",
    "t": "yarn test",
    "ts": "yarn run test-simple",
    "tsv": "yarn run test-simple --verbose",
    "u": "yarn upgrade --latest --force",
    "i": "yarn install --frozen-lockfile",
    "it": "yarn run i && yarn run t"
  },
  "homepage": "https://github.com/blackflux/lambda-serverless-api#readme",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/lambda-serverless-api/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/lambda-serverless-api.git"
  },
  "bugs": {
    "url": "https://github.com/blackflux/lambda-serverless-api/issues"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "aws-sdk-wrap": "4.5.1",
    "joi-strict": "1.2.6",
    "lambda-async": "1.0.9",
    "lambda-monitor-logger": "2.4.6",
    "lodash.clonedeep": "4.5.0",
    "lodash.difference": "4.5.0",
    "lodash.get": "4.4.2",
    "lodash.set": "4.3.2",
    "lru-cache-ext": "1.2.3",
    "object-fields": "2.0.17",
    "object-hash-strict": "1.2.34",
    "object-scan": "13.6.6",
    "painless-version": "1.2.6",
    "route-recognizer": "0.3.4",
    "swagger-parser": "10.0.2"
  },
  "peerDependencies": {},
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
